
@{
    ViewData["Title"] = "GetAll";
}

@model IEnumerable<AlpataEntities.Concrete.Meeting>

<h1>Meetings</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Start Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var meet in Model)
        {
            <tr>
                <td>@meet.Name</td>
                <td>@meet.Description</td>
                <td>@meet.EndDate</td>
            </tr>
        }
    </tbody>
</table>

